<%- include("partials/header") %>
<h1>Home</h1>
<p><%= startingContent %></p>
<% posts.forEach((post) => { %>
  <% let truncatedString = post.content.substring(0, 100) + " ...";%>
  <h1><%= post.title %></h1>
  <p>
    <%= truncatedString %>
    <a href="/posts/<%= post.title %>">Read more</a>
  </p>

<% }); %>
<%- include("partials/footer") %>
